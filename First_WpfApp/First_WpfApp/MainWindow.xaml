<Window x:Class="First_WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:First_WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Background="LightGray">
            <Rectangle Fill="CornflowerBlue" Height="50" ></Rectangle>
            <Rectangle Fill="CornflowerBlue" Height="50" Margin="20"></Rectangle>
        </StackPanel>
        <WrapPanel>
            <Rectangle Fill="AntiqueWhite" Height="50" Width="20" ></Rectangle>
            <Rectangle Fill="AntiqueWhite" Height="50" Width="20" Margin="10"></Rectangle>
        </WrapPanel>

        <Button Name="btnclick" Height="50" Width="200" Background="AliceBlue" Content="Click Me" Click="btnclick_Click"> </Button>
    </Grid>
</Window>
